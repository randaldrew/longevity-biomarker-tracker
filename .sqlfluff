[sqlfluff]
dialect = mysql
templater = raw
exclude_rules = LT02,L034,L036,L044,L046,CV01,CV02,PRS

[sqlfluff:indentation]
indent_unit = space
tab_space_size = 4

[sqlfluff:rules:L010]
capitalisation_policy = consistent

[sqlfluff:rules:L014]
extended_capitalisation_policy = consistent

[sqlfluff:rules:L030]
capitalisation_policy = consistent

[sqlfluff:rules:L040]
capitalisation_policy = consistent
